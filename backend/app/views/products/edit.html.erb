


<%= simple_form_for @product do |f| %>
  <%= f.input :name %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :characteristic %>
  <%= f.input :mark %>
  <%= f.input :kind %>
  <%= f.input :discount %>
  <%= f.input :reference %>
  <%= f.input :photos %>
  <%= f.button :submit %>
<% end %>
